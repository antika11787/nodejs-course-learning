<%- include('includes/head.ejs') %>
<link rel="stylesheet" href="/users.css" />
</head>
<body>
  <%- include('includes/navigation.ejs') %>
  <main>
    <h1>User list</h1>
    <% if (users.length > 0) { %>
    <div class="user-list">
      <% users.forEach((user)=> { %>
      <div class="user-list__item">
        <p class="user-list__username">Username: <%= user.username %></p>
      </div>
      <% }) %>
    </div>
    <% } %>
  </main>
  <%- include('includes/end.ejs') %>
</body>
